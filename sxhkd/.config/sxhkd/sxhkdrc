# Quicklaunch applications
alt + o; {i,z}
	{google-chrome-stable, zathura} 

alt + o; {m} 
 {google-chrome-stable www.gmail.com}
 
alt + o; {t} 
  {google-chrome-stable https://bit.ly/2P9BAEL}

alt + o; {c}
  {google-chrome-stable https://calendar.google.com/calendar/r}
alt + o; {n}
  {google-chrome-stable https://bit.ly/2xykj2h}
ctrl + alt + l;
  {slock}

# Audiokeys
XF86AudioMute
	amixer sset Master toggle
XF86Audio{Raise,Lower}Volume
	amixer sset Master 4%{+,-}
XF86Audio{Next,Prev}
	mpc {next,prev}
XF86Audio{Pause,Play,Stop}
	mpc {pause,play,stop}
